		numberOfStreams = Tokens[tokenIndex].size;        

    	FOR (i = 1 TO numberOfStreams)
		{
			Read();

			CSWITCH (streamType)
			{
            	CASE(1):
				{
					Process_AudioStream(1);
                	BREAK;
				}
            	CASE(2):
				{
					Process_VideoStream((i>1));
                	BREAK;
				}
            	CASE(3):
				{
					Process_SubtitleStream();
                	BREAK;
				}
			}				
		}
	}
	ELSE
		GenerateUserError("Process_AVStreams: Not an Array.");

}

